{
  "manifest_version": 2,
  "name": "Hello World Extension",
  "version": "1.0",
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
   "web_accessible_resources": [
    "node_modules/*"
  ],
  "content_scripts" : [
    {
      "matches": ["https://mail.google.com/*", "https://inbox.google.com/*"],
      "js": ["firebase.js", "inboxsdk.js", "bundle.js"]
    }
  ]
}